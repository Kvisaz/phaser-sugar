{
  "name": "@kvisaz/phaser-sugar",
  "version": "1.0.1",
  "description": "Phaser 3 utils and syntax sugar",
  "author": "Kvisaz Games",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "build": "webpack --config webpack.prod.js",
    "storybook": "webpack serve --mode development --config webpack.storybook.js",
    "build-storybook": "webpack --mode production --config webpack.storybook.js",
    "predeploy": "npm run build-storybook",
    "deploy": "git add docs && git commit -m \"Deploy docs to GitHub Pages\" && git push"
  },
  "keywords": [
    "phaser", "phaser 3"
  ],
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^22.1.0",
    "@typescript-eslint/eslint-plugin": "^8.0.1",
    "@typescript-eslint/parser": "^8.0.1",
    "copy-webpack-plugin": "^12.0.2",
    "esbuild-loader": "^4.2.2",
    "eslint": "8.57.0",
    "html-webpack-plugin": "^5.6.0",
    "jest": "^29.7.0",
    "phaser": "3.80.1",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.4",
    "typescript": "4.5.5",
    "ts-loader": "9.2.6",
    "webfontloader": "^1.6.28",
    "webpack": "^5.93.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  }
}
